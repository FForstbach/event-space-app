





  <h1 class="text-center">Hello Mr. <%= current_user.last_name %></h1>
  <h3 class="text-center">these are your bookings</h3>

<div class="row">
  <div class="col-xs-12">
    <table class="table">
      <tbody>
        <% @bookings.each do |booking| %>
          <tr>
            <td><%= booking.venue.name %></td>
            <td><%= booking.venue.address %></td>
            <td><%= booking.start %></td>
            <td><%= booking.end %></td>
            <td><%= link_to "Delete Booking", booking_path(booking), method: "delete"  %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
